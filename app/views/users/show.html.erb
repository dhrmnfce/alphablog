<h1><%= @user.username %></h1>
<div class="row">
    <div="col-md-4 col-md-offset-4">
        <div align="center">
            <%= gravatar_for @user %>
        </div>
        <h4 align="center"><%= @user.username %>'s articles</h4>
        <div align="center"><%= will_paginate @user_articles %></div>
        <%= render 'articles/article', obj: @user_articles %>
        <div align="center"><%= will_paginate @user_articles %></div>
    </div>
</div>
